-- Split gam
-------------------------------------------------------------------------

relation valGamPop =
  view K =
    holes [ g: ValGam | | g.l: ValGam, g.g: ValGam ]
    judgespec g.l ++ g.g === g
    judgeuse ag (g.l,g.g) `=` gamPop (g)


          | knTy = tvarv -> knTy
          | tyVarMp.syn = tyVarMp.fun
      judge A : expr
          | tyVarMp.inh = tyVarMp.fun
          | tyVarMp.syn = tyVarMp.arg
      ---
      judge R : expr
          | ty = tyVarMp.arg ty
          | tyVarMp.syn = tyVarMp.arg
    view I1 =
